    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "sdfa.App",
            "projectName": "myserver"
        },
        {
            "type": "java",
            "name": "Launch Main",
            "request": "launch",
            "mainClass": "sdfa.Main",
            "projectName": "myserver",
            "args": "sdfa.Main --port 8080"
        }
    ]
}